#include <iostream>

int main()
{
    int a, b, c;
    
    std::cin>>a>>b>>c;
    
    int max = a;
    int min = b;
    
    if(b < min)
    min = b;
    if(c < min)
    min = c;
    
    if(b > max)
    max = b;
    if(c > max)
    max = c;
    
    std::cout<<"min-"<<min<<", max-"<<max;

    return 0;
}
